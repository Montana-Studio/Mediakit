# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /Montana-Studio/sitios/_MediaKit/htdocs/app/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /Montana-Studio/sitios/_MediaKit/htdocs/app/index.php [L]
RewriteRule ^post/([0-9]+)?/?([0-9]+)?/?$ /index.php?p=$1&page=$2 [QSA]
</IfModule>

# END WordPress